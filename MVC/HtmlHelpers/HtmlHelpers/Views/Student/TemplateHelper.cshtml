@model HtmlHelpers.Models.Student
@{
    ViewBag.Title = "TemplateHelper";
}

<h2>TemplateHelper</h2>

@using(Html.BeginForm())
            {
<table>
    <tr>
        <td>@Html.DisplayName("RollNumber")</td>
        <td>@Html.Editor("rno")</td>
    </tr>
    <tr>
        <td>@Html.DisplayNameFor(m => m.RollNo)</td>
        <td>@Html.EditorFor(m => m.RollNo)</td>
    </tr>
    <tr>
        <td>@Html.DisplayName("Name of the Student")</td>
        <td>@Html.Editor("name")</td>
    </tr>
    <tr>
        <td>@Html.LabelFor(m => m.Name)</td>
        <td>@Html.TextBoxFor(m => m.Name)</td>
    </tr>
</table>
            }